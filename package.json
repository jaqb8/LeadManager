{
  "name": "lead_manager",
  "version": "1.0.0",
  "description": "Full Stack app with Django, Django Rest Framework &amp; React",
  "main": "index.js",
  "scripts": {
    "client": "webpack --mode development --hot --watch ./frontend/src/index.js --output ./frontend/static/frontend/main.js",
    "build": "webpack --mode production ./frontend/src/index.js --output ./frontend/static/frontend/main.js",
    "dev": "concurrently \"python manage.py runserver\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaqb8/LeadManager.git"
  },
  "author": "Jakub Aniszewski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaqb8/LeadManager/issues"
  },
  "homepage": "https://github.com/jaqb8/LeadManager#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^5.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  }
}
